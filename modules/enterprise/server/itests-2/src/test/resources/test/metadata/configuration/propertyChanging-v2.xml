        xmlns:c="urn:xmlns:rhq-configuration">
  
   <platform name="myPlatform7">
         
      <resource-configuration>
      
         <!-- one and two have been map-properties -->
         
         <c:list-property name="one">
            <c:simple-property name="simple">
